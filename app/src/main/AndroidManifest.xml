        <activity
            android:screenOrientation="portrait"
            android:name="com.ta.emilia.kpsp.DataActivity"
            android:label="Daftar Klien"/>

        <activity
            android:screenOrientation="portrait"
            android:name="com.ta.emilia.kpsp.DataCariActivity"
            android:label="Daftar Klien"/>

        <activity
            android:screenOrientation="portrait"
            android:name="com.ta.emilia.kpsp.DataDetailActivity"
